{
  "name": "bcloud",
  "version": "4.0.0",
  "description": "Cross platform BaiduPan client",
  "scripts": {
    "build": "tsc -p src/web",
    "build:watch": "tsc -p src/web -w",
    "prestart": "npm run build",
    "start": "npm build:watch && npm run serve",
    "start-electron": "electron .",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
